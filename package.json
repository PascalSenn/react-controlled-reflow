{
  "name": "react-controlled-reflow",
  "version": "0.0.1",
  "description": "Recompose wrapper. Omit redundant react rerender based on properties",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "npm": "npm run clean && tsc -d -p . && mv ./src/index.js . && mv ./src/index.d.ts .",
    "clean": "rm -f ./src/index.js* ./__tests__/*.d.ts ./__tests__/*.js ./lcov.info  && rm -f -R ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PascalSenn/react-controlled-reflow.git"
  },
  "keywords": [
    "react",
    "rerender",
    "render",
    "reflow",
    "performance",
    "recompose"
  ],
  "author": "Pascal Senn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PascalSenn/react-controlled-reflow/issues"
  },
  "homepage": "https://github.com/PascalSenn/react-controlled-reflow#readme",
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "recompose": "^0.27.1"
  },
  "devDependencies": {
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/recompose": "^0.26.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
